    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#53575f",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Functions, Keywords, Storage, Members, Properties, Tags",
            "scope": [
                "keyword",
                "storage",
                "meta.tag",
                "meta.object.member",
                "meta.object.member",
                "support.variable.property",
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#ffb4ef"
            }
        },
        {
            "name": "Methods, Classes, Brackets, Builtins, Attributes",
            "scope": [
                "constant.language",
                "entity.name.function",
                "entity.name.type.class",
                "entity.name.type.function",
                "entity.other.attribute-name",
                "support.type.primitive",
                "support.type.builtin",
                "meta.attribute"
            ],
            "settings": {
                "foreground": "#d1a2ff"
            }
        },
        {
            "name": "Numbers, Booleans, Types",
            "scope": [
                "constant.numeric",
                "constant.language.boolean",
                "support.type",
                "entity.name.type"
            ],
            "settings": { "foreground": "#ffc37f" }
        },
        {
            "name": "Variables, Parameters",
            "scope": ["variable"],
            "settings": {
                "foreground": "#9ebdff"
            }
        },
        {
            "name": "Storage",
            "scope": ["storage"],
            "settings": {
                "foreground": "#ff8c8c"
            }
        },
        {
            "name": "Strings",
            "scope": ["string"],
            "settings": {
                "foreground": "#b3b8c2",
                "fontStyle": ""
            }
        },
        {
            "name": "Punctuation",
            "scope": ["punctuation", "keyword.operator.type.annotation"],
            "settings": {
                "foreground": "#848994",
                "fontStyle": "bold"
            }
        }
    ]
}
